{
  "name": "vvg-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port ${PORT}",
    "dev:no-basepath": "BASE_PATH= NEXT_PUBLIC_BASE_PATH= next dev --port ${PORT}",
    "dev:clean": "node scripts/dev/dev-clean.js",
    "dev:seed": "node scripts/dev/dev-with-seed.js",
    "prebuild-disabled": "./scripts/check-memory.sh",
    "build": "next build",
    "build:standalone": "npm run prebuild && next build && cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/",
    "start": "next start --port ${PORT}",
    "start:prod": "NODE_ENV=production next start --port ${PORT}",
    "start:staging": "NODE_ENV=staging next start --port ${PORT}",
    "lint": "next lint",
    "lint:strict": "next lint --max-warnings 0",
    "db:migrate": "tsx scripts/database/run-migrations.ts",
    "test:e2e": "node scripts/test-e2e-simple.js",
    "validate": "node scripts/validate-template.js",
    "validate:paths": "./scripts/validate-paths.sh",
    "validate:quick": "npm run lint:strict && npm run typecheck",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "typecheck:strict": "tsc --noEmit",
    "prepare": "npm run typecheck || true",
    "test:functional": "node scripts/test-functional.js",
    "test:api": "node scripts/test-api-responses.js",
    "test:content": "node scripts/test-page-content.js",
    "test:complete": "node scripts/test-complete.js",
    "test:ready": "./scripts/template-ready-check.sh",
    "test:db:infrastructure": "node tests/database/infrastructure-test.js",
    "test:db:connection": "node tests/database/connection-test.js",
    "test:db:repository": "node tests/database/repository-pattern-test.js",
    "test:db:all": "npm run test:db:infrastructure && npm run test:db:repository && npm run test:db:connection",
    "clean-reset": "./scripts/clean-reset.sh",
    "clean-reset:force": "./scripts/clean-reset.sh --force",
    "reset:staging": "./scripts/clean-reset.sh --skip-production",
    "reset:production": "./scripts/clean-reset.sh --skip-staging",
    "worktree:setup": "mkdir -p worktrees && git worktree add worktrees/production main && git worktree add -b staging worktrees/staging main",
    "worktree:install": "cd worktrees/production && npm install && cd ../staging && npm install",
    "production:start": "cd worktrees/production && npm run build && pm2 start ../../config/ecosystem/production.config.js",
    "staging:start": "cd worktrees/staging && npm run build && pm2 start ../../config/ecosystem/staging.config.js",
    "production:dev": "cd worktrees/production && npm run dev",
    "staging:dev": "cd worktrees/staging && PORT=3001 npm run dev"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.842.0",
    "@aws-sdk/s3-request-presigner": "^3.842.0",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-aspect-ratio": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-context-menu": "^2.2.4",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-hover-card": "^1.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-menubar": "^1.1.4",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-toggle": "^1.1.1",
    "@radix-ui/react-toggle-group": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.6",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "date-fns": "^3.0.0",
    "docx": "^9.5.1",
    "dotenv": "^17.0.1",
    "embla-carousel-react": "8.5.1",
    "form-data": "^4.0.3",
    "input-otp": "1.4.1",
    "lucide-react": "^0.454.0",
    "mammoth": "^1.9.1",
    "mysql2": "^3.14.0",
    "next": "15.2.4",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.4",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.1",
    "openai": "^5.8.2",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.2.0",
    "pdfjs-dist": "^4.10.30",
    "pdfkit": "^0.17.1",
    "puppeteer": "^24.16.1",
    "react": "^18",
    "react-day-picker": "8.10.1",
    "react-dom": "^18",
    "react-hook-form": "^7.54.1",
    "react-resizable-panels": "^2.1.7",
    "recharts": "latest",
    "sonner": "^1.7.1",
    "swr": "^2.3.4",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "tesseract.js": "^5.1.0",
    "vaul": "^0.9.6",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.2.4",
    "eslint-plugin-import": "^2.29.1",
    "ignore-loader": "^0.1.2",
    "postcss": "^8",
    "string-replace-loader": "^3.2.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
